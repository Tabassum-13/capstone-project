<!DOCTYPE html>
<html>
    <head>
        <title>Order Food</title>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
        <style>
            body{
                background-color: white;
            }
            #section-1{
                height:150px;
            }
            nav{
                display: flex;
                justify-content: flex-end;
                align-items: center;
                gap: 500px;
                width: 100%;
                height: 100px;
                background-color: black;
                padding: 10px;
                position: relative;
            }
            nav ul{
                display: flex;
                justify-content: flex-end;
                align-items: center;
                gap:0 35px;
                font-size: 1.2rem;
                margin-left: 20px;
            }
            nav a{
                color: white;
                text-decoration: none;
                font-weight: 700;
            }
            nav a:hover{
                border-bottom: 2px solid hsl(0,0%,100%);
                transition: 1s;
            }
            .logo {
                border-radius: 50%; 
                width: 120px;
                height: auto; 
                position: absolute;
                top: 0px; 
                left: 10px; 
            }
            #section-2{
                height:150px;
            }
            .scroll-container {
                white-space: nowrap; 
                overflow-x: scroll; 
                border: 1px solid #ccc;
                padding: 10px;
                width: 80%;
            }
            .scroll-item {
                display: inline-block; 
                margin-right: 10px; 
                width: 100px; 
                height: 100px;
                border: 1px solid #ccc;
                border-radius: 50%; 
                overflow: hidden; 
            }
            .scroll-item img {
                max-width: 100%;
                max-height: 100%;
            }
            .scroll-item-label {
                font-size: 14px;
                margin-top: 5px;
            }
            .sidenav {
                height: 100%;
                width: 0;
                position: fixed;
                z-index: 1;
                top: 0;
                left: 0;
                background-color: #111;
                overflow-x: hidden;
                transition: 0.5s;
                padding-top: 60px;
            }
            .sidenav a {
                padding: 8px 8px 8px 32px;
                text-decoration: none;
                font-size: 25px;
                color: #818181;
                display: block;
                transition: 0.3s;
            }
            .sidenav a:hover {
                color: #f1f1f1;
            }
            .sidenav .closebtn {
                position: absolute;
                top: 0;
                right: 25px;
                font-size: 36px;
                margin-left: 50px;
            }
            #section-3{
                height:1000px;
            } 
            * {
                padding: 0;
                margin: 0;
                box-sizing: border-box;
                font-family: 'Poppins', sans-serif;
            } 
            .container {
                background-color: #ffffff;
                font-size: 16px;
                padding: 3em 2.8em;
                width: 80vw;
                margin: 0 auto;
                text-align: center;
                border-radius: 0.9em;
                box-shadow: 0 1.2em 2.5em rgba(42, 32, 0, 0.2);
            }
            .search-container {
                width: 100%;
                display: grid;
                grid-template-columns: 9fr 3fr;
                gap: 1.2em;
            }
            .search-container input {
                font-size: 1em;
                padding: 0.6em;
                border: none;
                outline: none;
                border-bottom: 2px solid #202030;
            }
            .search-container button {
                font-size: 1em;
                background-color: #f4c531;
                border: none;
                font-weight: 600;
                border-radius: 0.3em;
                color: #ffffff;
            }
            .search-container button:hover {
                font-size: 1em;
                background-color: white;
                border: 2px solid;
                border-color: #333;
                font-weight: 600;
                border-radius: 0.3em;
                color: #333;
            }
            #content {
                display: block;
                width: 30%;
                margin: 1.8em auto 0 auto;
            }
            .details {
                background-color: rgb(0,0,0);
                color: #ffffff;
                margin-top: 1.8em;
                text-align: center;
                padding: 0 .6em 0;
            }
            .details h2 {
                font-size: 1.2em;
                font-weight: 600;
            }
            .details h4 {
                font-size: 1em;
                font-weight: 400;
            }
            #show-recipe {
                font-size: 1em;
                position: relative;
                left: 45%;
                padding: 0.9em 0.6em;
                background-color: #f4c531;
                border: none;
                top: 1.5em;
                border-radius: 0.3em;
                font-weight: 600;
                color: #ffffff;
            }
            #show-recipe:hover {
                font-size: 1em;
                background-color: white;
                border: 2px solid;
                border-color: #333;
                font-weight: 600;
                border-radius: 0.3em;
                color: #333;
            }
            #recipe {
                background-color: #ffffff;
                border-radius: 0.6em;
                display: none;
                position: fixed; 
                top: 50%;
                left: 50%; 
                transform: translate(-50%, -50%); 
                z-index: 2;
                max-width: 90%; 
                padding: 20px; 
                box-shadow: 0 0 10px rgba(0, 0, 0, 0.2); 
            }
            #instructions {
                white-space: pre-wrap;
                word-wrap: break-word;
                font-size: 16px; 
                color: #333; 
                text-align: left; 
            }
            #hide-recipe {
                position: absolute; 
                top: 10px; 
                right: 10px; 
                background-color: #f4c531;
                border: none;
                border-radius: 50%; 
                width: 30px; 
                height: 30px; 
                color: #fff;
                text-align: center;
                line-height: 30px; 
                cursor: pointer;
                font-weight: bold;
            }
            #hide-recipe:hover {
                background-color: #333;
            }
            #recipe pre {
                white-space: pre-wrap;
                word-wrap: break-word;
                padding: 2.5em 1.2em 1.2em 1.2em;
                font-size: 0.92em;
                color: #303040;
            }
            #hide-recipe {
                font-size: 1em;
                position: relative;
                width: 1.8em;
                height: 1.8em;
                background-color: #f4c531;
                border: none;
                top: 1.0em;
                left: 50%;
                border-radius: 0.3em;
                color: #ffffff;
            }
            #hide-recipe:hover {
                font-size: 1em;
                background-color: white;
                border: 2px solid;
                border-color: #333;
                font-weight: 600;
                border-radius: 0.3em;
                color: #333;
            }
            ul {
                font-size: 1em;
                position: relative;
                display: grid;
                grid-template-columns: auto auto;
                gap: 0.8em 1.1em;
                padding: 1.2em 0 0 1.2em;
                color: #303040;
                text-transform: capitalize;
            }
            h3 {
                text-align: center;
                margin-top: 1.8em;
                color: #202030;
            }
            @media screen and (max-width: 500px) {
                .container {
                    font-size: 14px;
                }
            }
        </style>

        <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600&display=swap" rel="stylesheet">

    </head>
    <body>
        <div id="mySidenav" class="sidenav">
            <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
            <li>
                <a href="#">
                    <i class="fa fa-home" aria-hidden="true"></i>
                    <span>Home</span>
                </a>
            </li>
            <li>
                <a href="#">
                    <i class="fa fa-heart" aria-hidden="true"></i>
                    <span>Favourites</span>
                </a>
            </li>
            <li>
                <a href="#">
                    <i class="fa fa-info-circle" aria-hidden="true"></i>
                    <span>support</span>
                </a>
            </li>
            <li>
                <a href="#">
                    <i class="fa fa-cog" aria-hidden="true"></i>
                    <span>Setting</span>
                </a>
            </li>
            <li>
                <a href="home">
                    <i class="fa fa-power-off" aria-hidden="true"></i>
                    <span>Logout</span>
                </a>
            </li>
        </ul>
          </div>
        <section id="section-1">
            <nav>
                <span style="font-size:30px;cursor:pointer;color:white" onclick="openNav()">
                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSKdD0Q0ftNOIF69928SibexXx_TAvsjRAXbjcWEAGDuVp-JTU2_E0SiPPCmJQEh_j36Ao&usqp=CAU" class="logo">
                </span>
                <ul>
                    <li><a href="#">Contact</a></li>
                    <li><a href="#">Wishlist</a></li>
                    <li><a href="#">Search</a></li>
                    <li><a href="home">Logout</a></li>
                    
                </ul>
            </nav>
        </section>

        <section id="section-2">
            <center>
                <h2>Popular Cuisines</h2>
                <div class="scroll-container">
                    <div class="scroll-item">
                        <img src="https://healux.in/wp-content/uploads/2020/11/ChickenBiryani.jpg" alt="Content 1">
                        <div class="scroll-item-label">Biriyani</div>
                    </div>
                    <div class="scroll-item">
                        <img src="https://www.tiffinboxfood.com/wp-content/uploads/2020/10/vegetable-roll.jpg" alt="Content 2">
                        <div class="scroll-item-label">Rolls</div>
                    </div>
                    <div class="scroll-item">
                        <img src="https://static.toiimg.com/thumb/53110049.cms?width=1200&height=900" alt="Content 2">
                        <div class="scroll-item-label">Pizza</div>
                    </div>
                    <div class="scroll-item">
                        <img src="https://insanelygoodrecipes.com/wp-content/uploads/2020/02/Burger-and-Fries.jpg" alt="Content 2">
                        <div class="scroll-item-label">Burger</div>
                    </div>
                    <div class="scroll-item">
                        <img src="https://t4.ftcdn.net/jpg/02/54/92/05/360_F_254920579_xOIyVqIWxgUi0fSQ8FwBC95YlIVZpuCd.jpg" alt="Content 2">
                        <div class="scroll-item-label">Sweets</div>
                    </div>
                    <div class="scroll-item">
                        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTELLr6SYe9zIxtnS3lRTqwdjYcLGzrUxqwAw&usqp=CAU" alt="Content 2">
                        <div class="scroll-item-label">Milk Shakes</div>
                    </div>
                    <div class="scroll-item">
                        <img src="https://pic.warmoven.in/catalog/product/cache/4e14bcb566d25893ae7201d4dbdc22fd/image/234187ae/strawberry-cake.jpg" alt="Content 2">
                        <div class="scroll-item-label">Cake</div>
                    </div>
                    <div class="scroll-item">
                        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSN6QCo8uVNBEeK9GV1XLcpvpgUAhlkCMOJwQ&usqp=CAU" alt="Content 2">
                        <div class="scroll-item-label">Ice Creams</div>
                    </div>
                    <div class="scroll-item">
                        <img src="https://restaurantindia.s3.ap-south-1.amazonaws.com/s3fs-public/styles/1200x706/public/content6079.jpg?itok=wM692drs" alt="Content 2">
                        <div class="scroll-item-label">North India</div>
                    </div>
                    <div class="scroll-item">
                        <img src="https://gumlet.assettype.com/homegrown/import/book/kuhwlbayxs-1533040300.jpg?w=1200&h=675&auto=format%2Ccompress&fit=max&enlarge=true" alt="Content 2">
                        <div class="scroll-item-label">Soutn India</div>
                    </div>
                    <div class="scroll-item">
                        <img src="https://recipes.timesofindia.com/thumb/83740315.cms?width=1200&height=900" alt="Content 2">
                        <div class="scroll-item-label">Sandwich</div>
                    </div>
                    <div class="scroll-item">
                        <img src="data:image/jpeg;base64," alt="Content 2">
                        <div class="scroll-item-label">Desserts</div>
                    </div>
                
            </center>
           
        </section>


        <section id="section-3">
            <center>
                <div class="container">
                    <div class="search-container">
                        <input type="text" placeholder="Type a Dish Name Here..." id="user-inp">
                        <button id="search-btn">Search</button>
                    </div>
                    <div id="result"></div>
                </div>

            </center>    
        </section>


        <script>
            function openNav() {
                document.getElementById("mySidenav").style.width = "250px";
            }

            function closeNav() {
                document.getElementById("mySidenav").style.width = "0";
            }


            function searchFood() {
             var userInput = document.getElementById("foodInput").value;
            alert("Searching for: " + userInput);
            }

    
            let result = document.getElementById("result");
            let searchBtn = document.getElementById("search-btn");
            let url = "https://www.themealdb.com/api/json/v1/1/search.php?s=";

            searchBtn.addEventListener("click", () => {
                let userInp = document.getElementById("user-inp").value;
                if (userInp.length == 0) {
                    result.innerHTML = `<h3>Input Field Cannot Be Empty</h3>`;
                } else {
                fetch(url + userInp)
                .then((response) => response.json())
                .then((data) => {
                let myMeal = data.meals[0];
                console.log(myMeal);
                console.log(myMeal.strMealThumb);
                console.log(myMeal.strMeal);
                console.log(myMeal.strArea);
                console.log(myMeal.strInstructions);
                let count = 1;
                let ingredients = [];
                for (let i in myMeal) {
                    let ingredient = "";
                    let measure = "";
                    if (i.startsWith("strIngredient") && myMeal[i]) {
                        ingredient = myMeal[i];
                        measure = myMeal[`strMeasure` + count];
                        count += 1;
                        ingredients.push(`${measure} ${ingredient}`);
                    }
                }
                console.log(ingredients);

            result.innerHTML = `
            <img id="content" src=${myMeal.strMealThumb}>
            <div class="details">
                <h2>${myMeal.strMeal}</h2>
                <h4>${myMeal.strArea}</h4>
            </div>
            <div id="ingredient-con"></div>
            <div id="recipe">
                <button id="hide-recipe">X</button>
                <pre id="instructions">${myMeal.strInstructions}</pre>
            </div>
            <button id="show-recipe">View Recipe</button>
    `
            let ingredientCon = document.getElementById("ingredient-con");
            let parent = document.createElement("ul");
            let recipe = document.getElementById("recipe");
            let hideRecipe = document.getElementById("hide-recipe");
            let showRecipe = document.getElementById("show-recipe");

            ingredients.forEach((i) => {
            let child = document.createElement("li");
            child.innerText = i;
            parent.appendChild(child);
            ingredientCon.appendChild(parent);
        });

        hideRecipe.addEventListener("click", () => {
          recipe.style.display = "none";
        });
        showRecipe.addEventListener("click", () => {
          recipe.style.display = "block";
        });
      })
      .catch(() => {
        result.innerHTML = `<h3>Invalid Input</h3>`;
      });
  }
});

        </script>

    </body>
</html> 